{# Include this template inside the page.html.twig in drupal. #}
{% block header %}
  {% include '@organisms/header/header.twig' with {menu: primary_menu} only%}
{% endblock header %}

<main class="main-content">
  {% block main %}
    <p>Main content</p>
  {% endblock %}
</main>

{% block footer %}
  {% embed "@organisms/footer/footer.twig" with {menu: footer_menu} %}
    {% block slogan %}
      {{ 'Wingsuit flyer' }}
    {% endblock %}
  {% endembed %}
{% endblock footer %}
